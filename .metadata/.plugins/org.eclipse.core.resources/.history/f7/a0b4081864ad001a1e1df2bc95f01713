package prueba2;

class fase1 {

		// TODO Auto-generated method stub
	public fase1() {
		char [] letrasNombre;
		letrasNombre = new char [6];
		letrasNombre[0] = 'B';
		letrasNombre[1] = 'R';
		letrasNombre[2] = 'A';
		letrasNombre[3] = 'H';
		letrasNombre[4] = 'I';
		letrasNombre[5] = 'M';
	}
	static void ejecutar() {
		for (char letra: letrasNombre) {
			System.out.print(letra);
		}
	}
		



}
